{
    "compilerOptions": {
        "module": "nodenext",
        "moduleResolution": "NodeNext",
        "esModuleInterop": true,
        "downlevelIteration": true,
        "allowImportingTsExtensions": true,
        "strictNullChecks": true,
        "allowJs": true,
        "resolveJsonModule": true,
        "declaration": true,
        "emitDeclarationOnly": true
    }
}
